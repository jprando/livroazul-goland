    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "args": [
                "123",
                "456",
                "789"
            ],
            "env": {
                "GOFLAGS": "-tags=vscode"
            },
            "buildFlags": "-tags=vscode"
        },
        {
            "name": "Attach to Go process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": "${command:pickProcess}"
        }
    ]
}